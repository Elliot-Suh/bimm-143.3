x <- c(100,100,100,100,100,100,90, 100)
m <- c(80, 89, 57, 100, 85, 95, 100)
drop_low <- function(x){
s1 <- x
s1.1 <-(order(x, decreasing = TRUE))
s1.2 <- s1[s1.1]
s1.3 <- s1.2[1:(length(s1.2)-1)]
mean(s1.3)

print(mean(s1.3))
}
drop_low(m)
